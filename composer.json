{
    "name": "bmatovu/laravel-mtn-momo",
    "description": "Laravel MTN Momo",
    "homepage": "https://github.com/mtvbrianking/laravel-mtn-momo",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "package",
        "mtn",
        "momo",
        "api"
    ],
    "authors": [
        {
            "name": "Brian Matovu",
            "email": "mtvbrianking@gmail.com",
            "homepage": "http://bmatovu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.3.0"
    },
    "autoload": {
        "psr-4": {
            "Bmatovu\\MtnMomo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bmatovu\\MtnMomo\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "doc": "phpunit -d src -t docs",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    }
}
